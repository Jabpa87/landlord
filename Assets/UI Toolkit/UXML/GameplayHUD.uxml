                <ui:Button name="TradeButton" text="Trade" class="gameplay-hud-action-btn gameplay-hud-btn-green" style="background-image: url(&quot;project://database/Assets/Sprites/button.png?fileID=2800000&amp;guid=b2c659062aad75e47a6c934d5ab4bd57&amp;type=3#button&quot;);" />
