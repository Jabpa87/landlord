                        <ui:Button name="GameLogCloseButton" text="Close" class="gameplay-hud-log-action-btn gameplay-hud-log-action-close" style="background-image: url(&quot;project://database/Assets/Sprites/Gold%20Button.png?fileID=2800000&amp;guid=ede0ae4f446efc54196b89973a3304f6&amp;type=3#Gold Button&quot;);" />
