                <ui:Button name="SettingsFontSmall" text="Small" class="monopoly-btn monopoly-btn-secondary settings-font-btn" style="min-width: 90px;" />
                <ui:Button name="SettingsFontMedium" text="Medium" class="monopoly-btn monopoly-btn-secondary settings-font-btn" style="min-width: 90px;" />
                <ui:Button name="SettingsFontLarge" text="Large" class="monopoly-btn monopoly-btn-secondary settings-font-btn" style="min-width: 90px;" />
            </ui:VisualElement>
            <ui:Label text="Music" class="monopoly-text" style="font-size: 20px; margin-bottom: 8px;" />
            <ui:VisualElement name="SettingsMusicRow" style="flex-direction: row; align-items: center; margin-bottom: 20px;">
                <ui:Toggle name="SettingsMusicToggle" label="Background music" class="monopoly-toggle" style="flex-grow: 1;" />
            </ui:VisualElement>
            <ui:Button name="SettingsCloseButton" text="Close" class="monopoly-btn monopoly-btn-secondary" style="align-self: center; min-width: 140px;" />
