    -unity-font-style: bold;
    -unity-text-align: middle-left;
    margin-top: 2px;
}

.gameplay-hud-more-btn {
    height: 32px;
    min-width: 70px;
    font-size: 14px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    color: white;
    border-radius: 8px;
    border-width: 1px;
    border-color: rgb(198, 164, 79);
}

.gameplay-hud-indicators {
