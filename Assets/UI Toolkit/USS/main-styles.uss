    flex-direction: column;
    align-items: stretch;
    min-width: 0;
}

.news-item-username {
    font-size: 26px;
    -unity-font-style: bold;
    color: rgb(255, 215, 0);
    -unity-text-align: middle-left;
    margin-bottom: 2px;
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
}

.news-item-message {
    font-size: 26px;
    -unity-font-style: bold;
    color: rgba(255, 255, 255, 0.92);
    -unity-text-align: middle-left;
    -unity-text-overflow-position: end;
    white-space: normal;
    flex-grow: 1;
    width: 100%;
}

/* ========== Gameplay HUD Redesign (clean top row + floating roll + readable log) ========== */
.gameplay-hud-modern {
    position: absolute;
    left: 0;
    top: 50%;
    right: 0;
    bottom: auto;
    height: 20%;
    min-height: 150px;
    display: flex;
    flex-direction: column;
    align-items: stretch;
    padding: 6px 10px;
    gap: 6px;
    overflow: hidden;
    background-color: rgba(12, 38, 24, 0.35);
    border-top-width: 2px;
    border-top-color: rgba(198, 164, 79, 0.85);
    border-bottom-width: 1px;
    border-bottom-color: rgba(198, 164, 79, 0.45);
}

.gameplay-hud-button-bar {
    min-height: 48px;
    padding: 6px 8px;
    gap: 8px;
    border-radius: 14px;
    background-color: rgba(20, 70, 40, 0.55);
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.7);
    justify-content: space-evenly;
}

.gameplay-hud-button-bar .gameplay-hud-action-btn {
    min-width: 122px;
    height: 40px;
    font-size: 14px;
    margin-left: 0;
    margin-right: 0;
}

.gameplay-hud-status-row {
    flex-direction: row;
    align-items: center;
    gap: 10px;
    padding: 4px 10px;
    border-radius: 12px;
    background-color: rgba(12, 38, 24, 0.72);
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.4);
}

.gameplay-hud-status-label {
    font-size: 12px;
    color: rgba(242, 248, 242, 0.9);
    -unity-text-align: middle-left;
}

.gameplay-hud-status-primary {
    color: rgb(255, 225, 120);
    -unity-font-style: bold;
}

.gameplay-hud-status-money {
    color: rgb(132, 241, 152);
    -unity-font-style: bold;
}

.gameplay-hud-status-warning {
    color: rgb(255, 201, 120);
}

.gameplay-hud-profiles-row {
    background-color: rgba(18, 55, 28, 0.45);
    border-radius: 14px;
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.45);
    padding: 6px 8px;
    gap: 8px;
}

.gameplay-hud-profiles-row .gameplay-hud-player-panel {
    position: relative;
    left: auto;
    right: auto;
    top: auto;
    bottom: auto;
    width: auto;
    min-width: 150px;
    max-width: 320px;
    flex: 1 1 0;
    background-color: rgba(18, 55, 28, 0.92);
    border-color: rgba(198, 164, 79, 0.95);
    padding: 6px 8px;
    border-radius: 12px;
}

.gameplay-hud-profiles-row .gameplay-hud-avatar {
    width: 34px;
    height: 34px;
    min-width: 34px;
    min-height: 34px;
    margin-right: 8px;
}

.gameplay-hud-profiles-row .gameplay-hud-player-name {
    font-size: 13px;
}

.gameplay-hud-profiles-row .gameplay-hud-player-title {
    font-size: 11px;
}

.gameplay-hud-profiles-row .gameplay-hud-player-money {
    font-size: 12px;
}

.gameplay-hud-player-top-row {
    margin-bottom: 0;
}

.gameplay-hud-money-lane {
    min-height: 0;
    margin-top: 4px;
}

/* Pinned perk mini-card on each player profile */
.hud-perk-card-root {
    position: absolute;
    right: 6px;
    top: -16px;
    min-width: 120px;
    max-width: 165px;
    min-height: 44px;
    padding: 4px 6px;
    flex-direction: row;
    align-items: center;
    border-radius: 8px;
    background-color: rgba(34, 22, 10, 0.96);
    border-width: 1px;
    border-color: rgba(226, 194, 106, 0.92);
    box-shadow: 0 2px 6px rgba(0, 0, 0, 0.35);
}

.hud-perk-card-icon {
    width: 22px;
    height: 22px;
    min-width: 22px;
    min-height: 22px;
    margin-right: 6px;
    border-radius: 4px;
    background-color: rgba(255, 255, 255, 0.14);
    -unity-background-scale-mode: scale-to-fit;
}

.hud-perk-card-text-wrap {
    flex-direction: column;
    align-items: flex-start;
    justify-content: center;
    min-width: 0;
    flex-grow: 1;
}

.hud-perk-card-name {
    font-size: 10px;
    -unity-font-style: bold;
    color: rgb(255, 230, 145);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
    -unity-text-overflow-position: end;
}

.hud-perk-card-uses {
    margin-top: 1px;
    font-size: 9px;
    color: rgba(245, 245, 245, 0.9);
    white-space: nowrap;
}

.gameplay-hud-roll-floating {
    position: absolute;
    right: 18px;
    bottom: 10px;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    padding: 10px;
    border-radius: 12px;
    background-color: rgba(10, 35, 22, 0.7);
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.7);
    min-width: 150px;
}

.gameplay-hud-roll-floating .gameplay-hud-dice-label {
    margin-bottom: 6px;
    font-size: 18px;
}

.gameplay-hud-roll-floating .gameplay-hud-roll-btn {
    min-width: 140px;
    height: 48px;
    margin-bottom: 0;
    font-size: 20px;
}

.gameplay-hud-feed-section {
    left: 12px;
    right: 12px;
    top: 70%;
    bottom: 6px;
    height: auto;
    min-height: 160px;
    max-height: none;
    background-color: rgba(10, 28, 20, 0.9);
    border-top-width: 0;
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.6);
    border-radius: 14px;
    padding: 8px;
}

.gameplay-hud-feed-scroll {
    width: 100%;
}

.news-feed-item {
    background-color: rgba(24, 52, 33, 0.96);
    border-color: rgba(198, 164, 79, 0.55);
}

.news-item-username {
    font-size: 16px;
    color: rgb(255, 224, 122);
}

.news-item-message {
    font-size: 14px;
    color: rgba(245, 250, 245, 0.96);
    -unity-font-style: normal;
}

.gameplay-hud-log-button-row {
    position: absolute;
    right: 16px;
    top: 84%;
    z-index: 10;
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.gameplay-hud-log-button {
    min-width: 132px;
    height: 40px;
    background-color: rgb(28, 110, 56);
    color: white;
}

.gameplay-hud-log-overlay {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background-color: rgba(0, 0, 0, 0.65);
    align-items: center;
    justify-content: center;
    display: none;
}

.gameplay-hud-log-panel {
    width: 88%;
    max-width: 980px;
    height: 76%;
    max-height: 760px;
    background-color: rgb(15, 34, 24);
    border-width: 2px;
    border-color: rgb(198, 164, 79);
    border-radius: 14px;
    padding: 14px;
    flex-direction: column;
}

.gameplay-hud-log-header {
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
    margin-bottom: 10px;
}

.gameplay-hud-log-title {
    font-size: 22px;
    -unity-font-style: bold;
    color: rgb(246, 250, 246);
}

.gameplay-hud-log-actions {
    flex-direction: row;
    align-items: center;
    gap: 8px;
}

.gameplay-hud-log-action-btn {
    min-width: 90px;
    height: 36px;
    background-color: rgb(31, 98, 53);
    color: rgb(242, 247, 242);
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.65);
}

.gameplay-hud-log-action-close {
    background-color: rgb(130, 50, 50);
}

.gameplay-hud-log-scroll {
    flex-grow: 1;
    min-height: 200px;
}

.gameplay-hud-log-scroll .unity-label {
    color: rgb(236, 244, 236);
}

/* ========== Scrollbar Themes (HUD/feed/log) ========== */
/* Default active theme: dark emerald + gold accent (blends with current dark skin) */
.gameplay-hud-feed-scroll .unity-scroll-view__vertical-scroller,
.gameplay-hud-log-scroll .unity-scroll-view__vertical-scroller {
    width: 12px;
    min-width: 12px;
    background-color: rgba(8, 22, 16, 0.78);
    border-left-width: 1px;
    border-left-color: rgba(198, 164, 79, 0.35);
    border-radius: 8px;
    margin-left: 6px;
}

.gameplay-hud-feed-scroll .unity-base-slider__tracker,
.gameplay-hud-log-scroll .unity-base-slider__tracker {
    background-color: rgba(22, 62, 40, 0.85);
    border-radius: 8px;
}

.gameplay-hud-feed-scroll .unity-base-slider__dragger,
.gameplay-hud-log-scroll .unity-base-slider__dragger {
    background-color: rgba(198, 164, 79, 0.96);
    border-radius: 8px;
    border-width: 1px;
    border-color: rgba(255, 225, 145, 0.95);
    min-height: 34px;
}

.gameplay-hud-feed-scroll .unity-base-slider__dragger:hover,
.gameplay-hud-log-scroll .unity-base-slider__dragger:hover {
    background-color: rgba(228, 192, 95, 1);
}

.gameplay-hud-feed-scroll .unity-base-slider__dragger:active,
.gameplay-hud-log-scroll .unity-base-slider__dragger:active {
    background-color: rgba(255, 205, 92, 1);
}

/* Optional theme A: minimal dark (apply by adding class "scrollbar-theme-minimal") */
.scrollbar-theme-minimal .unity-scroll-view__vertical-scroller {
    width: 10px;
    min-width: 10px;
    background-color: rgba(14, 20, 18, 0.72);
}

.scrollbar-theme-minimal .unity-base-slider__tracker {
    background-color: rgba(35, 48, 42, 0.62);
    border-radius: 6px;
}

.scrollbar-theme-minimal .unity-base-slider__dragger {
    background-color: rgba(120, 155, 135, 0.9);
    border-radius: 6px;
    min-height: 30px;
}

/* Optional theme B: neon mint accent (apply by adding class "scrollbar-theme-neon") */
.scrollbar-theme-neon .unity-scroll-view__vertical-scroller {
    width: 12px;
    min-width: 12px;
    background-color: rgba(9, 18, 16, 0.85);
}

.scrollbar-theme-neon .unity-base-slider__tracker {
    background-color: rgba(18, 42, 35, 0.9);
    border-radius: 8px;
}

.scrollbar-theme-neon .unity-base-slider__dragger {
    background-color: rgba(58, 220, 180, 0.95);
    border-radius: 8px;
    border-width: 1px;
    border-color: rgba(170, 255, 235, 0.85);
    min-height: 34px;
}
