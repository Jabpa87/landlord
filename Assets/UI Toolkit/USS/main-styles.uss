    background-color: rgba(80, 80, 80, 0.8);
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.15);
}

/* Center cluster: current player, dice, wallet, Roll Dice, action buttons */
.gameplay-hud-center {
    position: absolute;
    left: 50%;
    top: 50%;
    translate: -50% -50%;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    min-width: 280px;
    padding: 20px;
    background-color: rgba(18, 40, 25, 0.85);
    border-radius: 16px;
    border-width: 2px;
    border-color: rgb(198, 164, 79);
