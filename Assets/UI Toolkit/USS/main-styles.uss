    flex-grow: 1;
    align-items: flex-start;
    justify-content: center;
}

.gameplay-hud-player-name {
    font-size: 16px;
    color: rgb(255, 215, 0);
    -unity-font-style: bold;
    -unity-text-align: middle-left;
}

.gameplay-hud-player-title {
    font-size: 13px;
    color: white;
    -unity-text-align: middle-left;
    margin-top: 2px;
}

.gameplay-hud-player-money {
    font-size: 15px;
