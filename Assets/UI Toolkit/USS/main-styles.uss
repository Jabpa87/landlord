/* ========== Gameplay HUD (new main HUD: 4 corners + center buttons) ========== */
.gameplay-hud-root {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

.gameplay-hud-safe-area {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
}

.gameplay-hud-main {
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    width: 100%;
    height: 100%;
}

/* Main HUD as column: button bar, then profiles row, then center (dice + roll + end turn) */
.gameplay-hud-main-column {
    display: flex;
    flex-direction: column;
    align-items: stretch;
}

/* Top bar: 6 buttons in one row (Menu, Build, Sell, Mortgage, Redeem, Trade) */
.gameplay-hud-button-bar {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex-wrap: nowrap;
    padding: 12px 16px;
    gap: 10px;
    flex-shrink: 0;
    background-color: rgba(220, 230, 245, 0.95);
    border-bottom-width: 2px;
    border-bottom-color: rgb(198, 164, 79);
}

/* Player profile cards in one horizontal row (2/3/4 by player count) */
.gameplay-hud-profiles-row {
    flex-direction: row;
    align-items: stretch;
    justify-content: center;
    flex-wrap: nowrap;
    gap: 12px;
    padding: 12px 16px;
    flex-shrink: 0;
}

/* Player panel when inside profiles row: no absolute position, equal flex so 2/3/4 cards share space */
.gameplay-hud-player-in-row {
    position: relative;
    flex: 1 1 0;
    min-width: 180px;
    max-width: 280px;
    width: auto;
}

/* Dice area: placeholder for Roll Dice button until dice visual is added */
.gameplay-hud-dice-area {
    flex-direction: column;
    align-items: center;
    justify-content: center;
    min-height: 160px;
    margin-bottom: 8px;
}

/* Player panels: dark green felt, gold border, corners. Size: width 100%, min 20%, max 30%, height auto (per main HUD layout). */
.gameplay-hud-player-panel {
    position: absolute;
    flex-direction: column;
    align-items: stretch;
    width: 100%;
    height: auto;
    min-width: 20%;
    max-width: 30%;
    min-height: auto;
    padding: 12px 14px;
    background-color: rgb(18, 55, 28);
    border-radius: 14px;
    border-width: 2px;
    border-color: rgb(198, 164, 79);
}

/* Row mode override: keep player cards visible in the profile strip */
.gameplay-hud-profiles-row .gameplay-hud-player-panel {
    position: relative;
    left: auto;
    right: auto;
    top: auto;
    bottom: auto;
    width: auto;
    min-width: 180px;
    max-width: 280px;
    flex: 1 1 0;
}

.gameplay-hud-player-top-row {
    flex-direction: row;
    align-items: center;
    margin-bottom: 10px;
}

.gameplay-hud-player-bottom-row {
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
}

.gameplay-hud-player-tl {
    left: 16px;
    top: 16px;
}

.gameplay-hud-player-tr {
    right: 16px;
    top: 16px;
    left: auto;
}

.gameplay-hud-player-bl {
    left: 16px;
    bottom: 16px;
    top: auto;
}

.gameplay-hud-player-br {
    right: 16px;
    bottom: 16px;
    left: auto;
    top: auto;
}

.gameplay-hud-avatar {
    width: 44px;
    height: 44px;
    min-width: 44px;
    min-height: 44px;
    border-radius: 8px;
    background-color: rgba(255, 255, 255, 0.2);
    border-width: 1px;
    border-color: rgba(198, 164, 79, 0.6);
    margin-right: 10px;
}

.gameplay-hud-avatar-circle {
