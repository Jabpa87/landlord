/* Main HUD Styles - Redesigned with Standard UI Style */
.main-hud {
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, 0);
    position: absolute;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    display: flex;
}

/* Top Panel - Card Style */
.main-hud-top-panel {
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    height: 180px;
    background-color: rgb(33, 43, 55);
    border-radius: 0 0 16px 16px;
    border-width: 0 0 2px 0;
    border-color: rgb(20, 28, 36);
    flex-direction: column;
}

.main-hud-top-header {
    padding: 12px 20px;
    background-color: rgb(18, 102, 173);
    border-top-left-radius: 0;
    border-top-right-radius: 0;
    border-bottom-width: 2px;
    border-bottom-color: rgb(10, 60, 110);
}

.main-hud-top-title {
    font-size: 24px;
    color: white;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
}

.main-hud-top-body {
    padding: 12px 20px;
    flex-direction: row;
    align-items: center;
    flex-grow: 1;
}

.main-hud-wallet-text {
    font-size: 20px;
    color: #4CAF50;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
}

.main-hud-supply-text {
    font-size: 14px;
    color: rgba(255, 255, 255, 0.75);
    -unity-text-align: middle-left;
}

.main-hud-doubles-text {
    font-size: 14px;
    color: #FFD700;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    margin-left: auto;
}

/* Bottom Panel - Card Style */
.main-hud-bottom-panel {
    position: absolute;
    bottom: 80px;
    left: 0;
    right: 0;
    height: 160px;
    background-color: rgb(33, 43, 55);
    border-radius: 16px 16px 0 0;
    border-width: 2px 0 0 0;
    border-color: rgb(20, 28, 36);
    flex-direction: row;
    align-items: center;
    justify-content: space-between;
    padding: 15px 20px;
}

/* Left and Right Player Sides */
.main-hud-players-side {
    flex-direction: column;
    align-items: flex-start;
    justify-content: center;
    width: 200px;
}

/* Individual Player Info */
.main-hud-player-info {
    flex-direction: row;
    align-items: center;
    width: 100%;
    padding: 8px 12px;
    background-color: rgba(0, 0, 0, 0.2);
    border-radius: 8px;
    border-width: 1px;
    border-color: rgba(255, 255, 255, 0.1);
}

.main-hud-player-avatar {
    width: 40px;
    height: 40px;
    border-radius: 20px;
    background-color: #555;
    border-width: 2px;
    border-color: white;
    flex-shrink: 0;
}

.main-hud-player-name {
    font-size: 14px;
    color: white;
    -unity-font-style: normal;
    -unity-text-align: middle-left;
    min-width: 70px;
}

.main-hud-player-money {
    font-size: 16px;
    color: #4CAF50;
    -unity-font-style: bold;
    -unity-text-align: middle-left;
    margin-left: auto;
}

/* Center Controls */
.main-hud-center-controls {
    flex-direction: column;
    align-items: center;
    justify-content: center;
    flex-grow: 1;
}

.main-hud-dice-container {
    flex-direction: row;
    align-items: center;
    justify-content: center;
}

.main-hud-dice-text {
    font-size: 18px;
    color: white;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

.main-hud-roll-button {
    width: 200px;
    height: 50px;
    font-size: 20px;
    background-color: rgb(50, 170, 70);
    color: white;
    border-radius: 10px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    border-width: 0;
}

.main-hud-roll-button:hover {
    background-color: rgb(45, 150, 60);
}

.main-hud-roll-button:active {
    background-color: rgb(40, 130, 50);
}

.main-hud-roll-button:disabled {
    background-color: rgb(90, 100, 110);
    color: rgba(255, 255, 255, 0.5);
}

/* Action Buttons Row - Footer Style */
.main-hud-action-buttons-row {
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    height: 70px;
    background-color: rgb(33, 43, 55);
    border-radius: 16px 16px 0 0;
    border-width: 2px 0 0 0;
    border-color: rgb(20, 28, 36);
    flex-direction: row;
    align-items: center;
    justify-content: center;
    padding: 10px;
}

.main-hud-action-btn {
    height: 45px;
    min-width: 100px;
    font-size: 14px;
    background-color: rgb(50, 170, 70);
    color: white;
    border-radius: 8px;
    -unity-font-style: bold;
    -unity-text-align: middle-center;
    border-width: 0;
}

.main-hud-action-btn:hover {
    background-color: rgb(45, 150, 60);
}

.main-hud-action-btn:active {
    background-color: rgb(40, 130, 50);
}

.main-hud-action-btn:disabled {
    background-color: rgb(90, 100, 110);
    color: rgba(255, 255, 255, 0.5);
}

.main-hud-trade-btn {
    background-color: rgb(200, 60, 60);
}

.main-hud-trade-btn:hover {
    background-color: rgb(180, 50, 50);
}

.main-hud-trade-btn:active {
    background-color: rgb(160, 40, 40);
}

/* Button Styles */
.action-button {
    width: 200px;
    height: 60px;
    margin: 10px;
    font-size: 24px;
    background-color: #2196F3;
    color: white;
    border-radius: 8px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

.action-button:hover {
    background-color: #1976D2;
}

.action-button:active {
    background-color: #0D47A1;
}

.action-button:disabled {
    background-color: #666;
    color: #999;
}

/* Modal Panel Styles */
.modal-panel {
    position: absolute;
    width: 600px;
    min-height: 300px;
    left: 50%;
    top: 50%;
    margin-left: -300px; /* Half of width for centering */
    margin-top: -150px; /* Half of min-height for centering */
    background-color: rgba(30, 30, 30, 0.95);
    border-radius: 12px;
    padding: 30px;
    flex-direction: column;
    align-items: center;
    justify-content: center;
    border-width: 2px;
    border-color: #4CAF50;
}

.panel-title {
    font-size: 28px;
    color: white;
    margin-bottom: 20px;
    -unity-text-align: middle-center;
    -unity-font-style: normal;
    white-space: normal;
}

.primary-button {
    width: 250px;
    height: 50px;
    margin: 10px;
    font-size: 20px;
    background-color: #4CAF50;
    color: white;
    border-radius: 8px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

.primary-button:hover {
    background-color: #45a049;
}

.primary-button:disabled {
    background-color: #666;
    color: #999;
}

.secondary-button {
    width: 250px;
    height: 50px;
    margin: 10px;
    font-size: 20px;
    background-color: #666;
    color: white;
    border-radius: 8px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

.secondary-button:hover {
    background-color: #777;
}

/* Profile Picture */
.profile-picture {
    width: 80px;
    height: 80px;
    border-radius: 40px;
    background-color: #555;
    border-width: 2px;
    border-color: white;
}

/* Card Panel Specific */
.card-panel {
    width: 500px;
    min-height: 400px;
}

.card-title {
    font-size: 36px;
    color: #FFD700;
    -unity-font-style: bold;
    margin-bottom: 20px;
    -unity-text-align: middle-center;
}

.card-description {
    font-size: 20px;
    color: white;
    margin-bottom: 30px;
    -unity-text-align: middle-center;
    -unity-font-style: normal;
    white-space: normal;
}

.button-text {
    -unity-text-align: middle-center;
    -unity-font-style: normal;
    color: white;
}

/* Main Menu Styles */
.main-menu {
    width: 100%;
    height: 100%;
    background-color: rgba(20, 20, 30, 0.95);
    flex-direction: column;
    align-items: center;
    justify-content: flex-start;
    padding: 40px;
}

.menu-title {
    font-size: 48px;
    color: #FFD700;
    -unity-font-style: bold;
    margin-bottom: 30px;
    -unity-text-align: middle-center;
}

.menu-section {
    width: 90%;
    max-width: 800px;
    background-color: rgba(40, 40, 50, 0.8);
    border-radius: 12px;
    padding: 20px;
    margin-bottom: 20px;
    flex-direction: column;
    align-items: center;
}

.section-title {
    font-size: 24px;
    color: white;
    -unity-font-style: bold;
    margin-bottom: 15px;
    -unity-text-align: middle-center;
}

.button-row {
    flex-direction: row;
    align-items: center;
    justify-content: center;
    flex-wrap: wrap;
}

.count-button {
    width: 80px;
    height: 60px;
    font-size: 24px;
    background-color: #2196F3;
    color: white;
    border-radius: 8px;
    -unity-font-style: normal;
    -unity-text-align: middle-center;
}

.count-button:hover {
    background-color: #1976D2;
}

.count-button:active {
    background-color: #0D47A1;
}

.count-button.selected {
    background-color: #4CAF50;
    border-width: 3px;
    border-color: #FFD700;
}

.player-setup-scroll {
    width: 90%;
    max-width: 800px;
    max-height: 400px;
    margin-bottom: 20px;
}

.player-setup-container {
    flex-direction: column;
    padding: 10px;
}

.player-slot {
    background-color: rgba(50, 50, 60, 0.8);
    border-radius: 8px;
    padding: 15px;
    flex-direction: column;
    border-width: 2px;
    border-color: #555;
}

.player-slot-title {
    font-size: 20px;
    color: #FFD700;
    -unity-font-style: bold;
    margin-bottom: 5px;
    -unity-text-align: middle-left;
}

.player-row {
    flex-direction: row;
    align-items: center;
    width: 100%;
}

.player-label {
    font-size: 16px;
    color: white;
    min-width: 80px;
    -unity-text-align: middle-left;
}

.player-input {
    flex-grow: 1;
    height: 35px;
    font-size: 16px;
    background-color: rgba(30, 30, 40, 0.9);
    color: white;
    border-width: 1px;
    border-color: #666;
    border-radius: 4px;
    padding: 5px 10px;
}

.avatar-options {
    flex-direction: row;
    flex-wrap: wrap;
}

.avatar-button {
    width: 60px;
    height: 60px;
    border-radius: 30px;
    border-width: 2px;
    border-color: #666;
    background-color: #555;
}

.avatar-button:hover {
    border-color: white;
}

.avatar-button.selected {
    border-color: #FFD700;
    border-width: 3px;
}

.color-options {
    flex-direction: row;
    flex-wrap: wrap;
}

.color-button {
    width: 40px;
    height: 40px;
    border-radius: 20px;
    border-width: 2px;
    border-color: #666;
}

.color-button:hover {
    border-color: white;
}

.color-button.selected {
    border-color: #FFD700;
    border-width: 3px;
}

